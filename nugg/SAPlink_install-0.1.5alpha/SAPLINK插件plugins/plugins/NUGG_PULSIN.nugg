   <exception CLSNAME="ZSAPLINK_BADI_IMPLEMENTATIONS" CMPNAME="SAVE_IMPLEMENTATION" SCONAME="SAVE_FAILURE" VERSION="1" LANGU="E" MTDTYPE="0" EDITORDER="1 " AUTHOR="ABAP01" CREATEDON="20140226" CHANGEDON="00000000"/>
