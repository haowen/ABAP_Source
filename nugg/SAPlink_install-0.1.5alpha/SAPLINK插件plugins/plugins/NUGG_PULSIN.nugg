          when &apos;DEVCLASS&apos;. &quot;development class should always be $TMP
            sValue = &apos;$TMP&apos;.
          when others.
            sValue = &lt;fieldValue&gt;.
        endcase.
        if sValue is not initial.
          rc = Node-&gt;set_attribute( name = sName value = sValue ).
        endif.
      else.
* WHAT?&gt;!??
      endif.
    endloop.
  endmethod.
