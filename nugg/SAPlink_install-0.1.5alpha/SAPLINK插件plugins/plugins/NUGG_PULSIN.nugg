form uploadXMLFromLM using p_filename xmlString type string .
  data tempTable type table_of_strings.
  data l_fileName type string.

    l_fileName = p_filename.
    call method CL_GUI_FRONTEND_SERVICES=&gt;GUI_UPLOAD
          exporting
